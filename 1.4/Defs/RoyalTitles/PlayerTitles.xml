  <!-- King -->

  <TitularRoyalty.PlayerTitleDef ParentName="BasePlayerTitle">
    <defName>TitularRoyalty_T_RY_King</defName>
    <label>King</label>
    <labelFemale>Queen</labelFemale>
    <description>Ruler of your colony.</description>
    <seniority>1500</seniority>

    <permitPointsAwarded>1</permitPointsAwarded>

    <favorCost>100</favorCost>
    <canBeInherited>False</canBeInherited>
    <changeHeirQuestPoints>1000</changeHeirQuestPoints>
    <allowDignifiedMeditationFocus>True</allowDignifiedMeditationFocus>
    <awardWorkerClass>RoyalTitleAwardWorker_Instant</awardWorkerClass>
    <minExpectation>Royal</minExpectation>
    <grantedAbilities>
      <li>Speech</li>
    </grantedAbilities>
    <speechCooldown>900000</speechCooldown>
    <disabledWorkTags>
      <li>ManualDumb</li>
      <li>PlantWork</li>
      <li>Mining</li>
    </disabledWorkTags>
    <decreeTags>
      <li>All</li>
    </decreeTags>
    <disabledJoyKinds>
      <li>Gaming_Dexterity</li>
    </disabledJoyKinds>
    <foodRequirement>
      <minQuality>MealFine</minQuality>
      <allowedTypes>Processed, Liquor</allowedTypes>
      <allowedDefs>
        <li>Ambrosia</li>
        <li>Milk</li>
        <li>RawBerries</li>
      </allowedDefs>
    </foodRequirement>
    <throneRoomRequirements>
      <li Class="RoomRequirement_HasAssignedThroneAnyOf">
        <things>
          <li>GrandThrone</li>
        </things>
      </li>
      <li Class="RoomRequirement_Area">
        <area>8</area>
      </li>
      <li Class="RoomRequirement_Impressiveness">
        <impressiveness>45</impressiveness>
      </li>
    </throneRoomRequirements>

    <titleTier>Sovereign</titleTier>
    <TRInheritable>True</TRInheritable>
  </TitularRoyalty.PlayerTitleDef>

